---
title: Archive
---

<ul class="tags-list">
  {% for tag, posts in collections | dictsort %}
  {% if tag !== "all" and tag !== "post" %}
    <li><a href="/tags/{{tag}}/">{{tag}} <span aria-hidden="true">{{posts.length}}</span></a></li>
  {% endif %}
  {% endfor %}
</ul>

<ul>
  {% for post in collections.post | reverse  %}
      <li> <a href="{{post.url}}">{{ post.data.title }}</a></li>
  {% endfor %}
</ul>